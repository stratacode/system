#!/bin/sh

scp -r /jjv/StrataCode/build/doc_clientOnly/web/ scserver@strataCode.com:/tmp/newWeb.$$
scp -r /jjv/StrataCode/build/doc_webConfig/ scserver@strataCode.com:/tmp/newForms.$$
ssh scserver@strataCode.com mkdir -p /usr/local/scc/build/doc_clientOnly/web
ssh scserver@stratacode.com mv /usr/local/scc/build/doc_clientOnly/web /tmp/oldWeb.$$
ssh scserver@stratacode.com mv /usr/local/scc/build/doc_webConfig /tmp/oldForms.$$

ssh scserver@stratacode.com mv /tmp/newWeb.$$ /usr/local/scc/build/doc_clientOnly/web
ssh scserver@stratacode.com mv /tmp/newForms.$$ /usr/local/scc/build/doc_webConfig

ssh scserver@stratacode.com ln -s /usr/local/scc/packages /usr/local/scc/build/doc_clientOnly/web/packages
